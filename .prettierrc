{
  "trailingComma": "es5",
  "arrowParens": "always",
  "endOfLine": "lf",
  "tabWidth": 2,
  "semi": false,
  "singleQuote": true,
  "importOrder": [
    "^react(.*)$",
    "^[a-zA-Z](.*)$",
    "^@mui(.*)$",
    "^@[a-zA-Z](.*)$",
    "^@/[a-zA-Z](.*)$",
    "^./[a-zA-Z](.*)$",
    "^../[a-zA-Z](.*)$",
    "^../../(.*)$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
